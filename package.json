{
  "name": "WeDoRandom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios --scheme \"WeDoRandom.dev\"",
    "ios:release:debug": "react-native run-ios --scheme \"WeDoRandom.dev\" --configuration Release",
    "ios:debug:prod": " react-native run-ios --scheme \"WeDoRandom.prod\"",
    "ios:release:prod": " react-native run-ios --scheme \"WeDoRandom.prod\" --configuration Release",
    "android": "ENVFILE=.env.development react-native run-android",
    "android:release:dev": "ENVFILE=.env.development react-native run-android --variant release",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "android:release:prod": "ENVFILE=.env.production react-native run-android --variant release",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "clean": "watchman watch-del-all",
    "link": "react-native link && react-native unlink react-native-vector-icons",
    "pod": "cd ios && pod install",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "assembleRelease": "cd android && ./gradlew clean && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-clipboard/clipboard": "^1.11.0",
    "@react-native-firebase/app": "^15.4.0",
    "@react-native-firebase/firestore": "^15.4.0",
    "@react-navigation/bottom-tabs": "^6.3.3",
    "@react-navigation/elements": "^1.3.5",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/stack": "^6.2.3",
    "@redux-saga/types": "^1.2.1",
    "@types/react-redux": "^7.1.24",
    "@types/redux-logger": "^3.0.9",
    "deprecated-react-native-prop-types": "^2.3.0",
    "prettier": "^2.7.1",
    "react": "18.0.0",
    "react-i18next": "^11.18.5",
    "react-native": "0.69.5",
    "react-native-barcode-mask": "^1.2.4",
    "react-native-camera": "^4.2.1",
    "react-native-config": "^1.4.6",
    "react-native-device-info": "^10.2.0",
    "react-native-gesture-handler": "^2.6.0",
    "react-native-qrcode-svg": "^6.1.2",
    "react-native-reanimated": "^2.10.0",
    "react-native-safe-area-context": "^4.3.3",
    "react-native-screens": "^3.17.0",
    "react-native-svg": "^12.1.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.2",
    "realm": "^10.20.0",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.19.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.18",
    "@types/react-native": "^0.69.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
