{
  "name": "WeDoRandom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios --scheme \"WeDoRandom.dev\"",
    "ios:release:debug": "react-native run-ios --scheme \"WeDoRandom.dev\" --configuration Release",
    "ios:debug:prod": " react-native run-ios --scheme \"WeDoRandom.prod\"",
    "ios:release:prod": " react-native run-ios --scheme \"WeDoRandom.prod\" --configuration Release",
    "android": "ENVFILE=.env.development react-native run-android",
    "android:release:dev": "ENVFILE=.env.development react-native run-android --variant release",
    "android:prod": "ENVFILE=.env.production react-native run-android",
    "android:release:prod": "ENVFILE=.env.production react-native run-android --variant release",
    "start": "watchman watch-del-all && react-native start --reset-cache",
    "clean": "watchman watch-del-all",
    "link": "react-native link && react-native unlink react-native-vector-icons",
    "pod": "cd ios && pod install",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "bundleRelease": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "assembleRelease": "cd android && ./gradlew clean && ./gradlew assembleRelease"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@redux-saga/types": "^1.2.1",
    "@types/react-redux": "^7.1.24",
    "@types/redux-logger": "^3.0.9",
    "prettier": "^2.7.1",
    "react": "18.0.0",
    "react-native": "0.69.5",
    "react-native-config": "^1.4.6",
    "react-redux": "^8.0.2",
    "redux": "^4.2.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.18",
    "@types/react-native": "^0.69.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "react-test-renderer": "18.0.0",
    "typescript": "^4.8.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
